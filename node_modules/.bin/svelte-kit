#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/@sveltejs+kit@2.49.0_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.43.14_vite@7.2.4_@type_ec474b98e3917b8dc7575b2e146150cf/node_modules/@sveltejs/kit/node_modules:/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/@sveltejs+kit@2.49.0_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.43.14_vite@7.2.4_@type_ec474b98e3917b8dc7575b2e146150cf/node_modules/@sveltejs/node_modules:/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/@sveltejs+kit@2.49.0_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.43.14_vite@7.2.4_@type_ec474b98e3917b8dc7575b2e146150cf/node_modules:/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/@sveltejs+kit@2.49.0_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.43.14_vite@7.2.4_@type_ec474b98e3917b8dc7575b2e146150cf/node_modules/@sveltejs/kit/node_modules:/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/@sveltejs+kit@2.49.0_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.43.14_vite@7.2.4_@type_ec474b98e3917b8dc7575b2e146150cf/node_modules/@sveltejs/node_modules:/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/@sveltejs+kit@2.49.0_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.43.14_vite@7.2.4_@type_ec474b98e3917b8dc7575b2e146150cf/node_modules:/home/tdk/Documentos/DevJosh/svelte/acorde/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
